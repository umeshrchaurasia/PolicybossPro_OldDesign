<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_dashboard"
    android:orientation="vertical"
    tools:context=".ultralaksha.ultralakshya.fragment.quote.content.UltraLakshayProductCombo">

    <!-- TODO: Update blank fragment layout -->
    <TextView
        style="@style/TextViewStyle.Medium"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:gravity="center"
        android:paddingTop="5dp"
        android:paddingBottom="5dp"
        android:text="@string/UltraLakshyaProdCombo"
        android:textColor="@color/black"
        android:textStyle="bold" />

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"

        android:layout_marginLeft="4dp"
        android:layout_marginRight="4dp"
        android:orientation="vertical">
        <TableLayout
            android:id="@+id/tbParent"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:weightSum="100">

            <TableRow
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

                <TextView
                    style="@style/TextViewStyle.Small"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="25"
                    android:background="@drawable/cell_blue_shape"
                    android:gravity="center"

                    android:text="Product"
                    android:textColor="@color/white"></TextView>

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_blue_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="Term"
                    android:textColor="@color/white"></TextView>

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_blue_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="PPT"
                    android:textColor="@color/white"></TextView>


                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_blue_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="Mode"
                    android:textColor="@color/white"></TextView>


                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="15"
                    android:background="@drawable/cell_blue_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="Sum"
                    android:textColor="@color/white"></TextView>


                <TableLayout
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="30"
                    android:weightSum="100">

                    <TableRow>

                        <TextView
                            style="@style/TextViewStyle.Small"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="100"
                            android:background="@drawable/cell_blue_shape"
                            android:gravity="center"

                            android:text="Premium"
                            android:textColor="@color/white"></TextView>
                    </TableRow>


                    <TableRow>

                        <TextView
                            style="@style/TextViewStyle.VerySmall"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="50"
                            android:background="@drawable/cell_blue_shape"
                            android:gravity="center"
                            android:text="YEAR1"
                            android:paddingLeft="2dp"
                            android:paddingRight="2dp"
                            android:textColor="@color/white">

                        </TextView>

                        <TextView
                            android:id="@+id/txtOtherYear"
                            style="@style/TextViewStyle.VerySmall"
                            android:layout_width="0dp"
                            android:layout_height="match_parent"
                            android:layout_weight="50"
                            android:background="@drawable/cell_blue_shape"
                            android:gravity="center"
                            android:text="2 to 17"
                            android:paddingLeft="2dp"
                            android:paddingRight="2dp"
                            android:textColor="@color/white">

                        </TextView>

                    </TableRow>


                </TableLayout>

            </TableRow>

            <TableRow
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp">

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="25"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:textColor="@color/header_dark_text"
                    android:text="LIC \n Jeevan Lakshya"></TextView>

                <TextView
                    android:id="@+id/txtLicTerm"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="20"></TextView>

                <TextView
                    android:id="@+id/txtLicPPT"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="17"></TextView>


                <TextView
                    android:id="@+id/txtLicMode"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="YYL"></TextView>


                <TextView
                    android:id="@+id/txtLicSum"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="15"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="100000"></TextView>


                <TableRow
                    android:layout_weight="30"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txtLicPremYearOne"
                        style="@style/TextViewStyle.VerySmall"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:text="54798"
                        >

                    </TextView>

                    <TextView
                        android:id="@+id/txtLicPremOtherYears"
                        style="@style/TextViewStyle.VerySmall"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:text="894646"

                        >

                    </TextView>

                </TableRow>


            </TableRow>

            <TableRow
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp">

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="25"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:textColor="@color/header_dark_text"
                    android:text="HDFC\nClick2Protect 3D Plus"></TextView>

                <TextView
                    android:id="@+id/txtHdfcTerm"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="20"></TextView>

                <TextView
                    android:id="@+id/txtHdfcPPT"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="17"></TextView>


                <TextView
                    android:id="@+id/txtHdfcMode"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="YYL"></TextView>


                <TextView
                    android:id="@+id/txtHdfcSum"
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="15"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text="100000"></TextView>


                <TableRow
                    android:layout_weight="30"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txtHdfcPremYearOne"
                        style="@style/TextViewStyle.VerySmall"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:text="14798"
                        >

                    </TextView>

                    <TextView
                        android:id="@+id/txtHdfcPremOtherYears"
                        style="@style/TextViewStyle.VerySmall"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:text="194646"

                        >

                    </TextView>

                </TableRow>


            </TableRow>

            <TableRow
                android:layout_marginLeft="1dp"
                android:layout_marginRight="1dp"
                android:layout_height="wrap_content">
                <TextView
                    android:id="@+id/txtBuyNowHDFC"
                    style="@style/TextViewStyle.Small"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="25"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:textColor="@color/colorPrimaryDark"
                    android:text="@string/buyHdfc"></TextView>

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text=""></TextView>

                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text=""></TextView>


                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="10"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text=""></TextView>


                <TextView
                    style="@style/TextViewStyle.VerySmall"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:layout_weight="15"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:paddingLeft="2dp"
                    android:paddingRight="2dp"
                    android:text=""></TextView>


                <TableRow
                    android:layout_weight="30"
                    android:layout_width="0dp"
                    android:layout_height="match_parent">

                    <TextView
                        android:id="@+id/txtTotalOne"
                        style="@style/TextViewStyle.Small"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:textColor="@color/black"
                        android:text=""
                        >

                    </TextView>

                    <TextView
                        android:id="@+id/txtTotalTwo"
                        style="@style/TextViewStyle.Small"
                        android:layout_width="0dp"
                        android:layout_height="match_parent"
                        android:layout_weight="50"
                        android:background="@drawable/cell_shape"
                        android:gravity="center"
                        android:text=""
                        android:textColor="@color/black"
                        >

                    </TextView>

                </TableRow>


            </TableRow>


        </TableLayout>
    </LinearLayout>



</LinearLayout>
